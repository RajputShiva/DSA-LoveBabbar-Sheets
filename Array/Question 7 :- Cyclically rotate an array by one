


--------------------------------------------------------------------------------------------------------------------------------------------------------------------------
Example :-
Input:
N = 5
A[] = {1, 2, 3, 4, 5}
Output:
5 1 2 3 4

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------

public void rotate(int arr[], int n)
    {
       int x = arr[0];

    for(int i=1;i<n;i++){

        int temp = arr[i];

        arr[i] = x;

        x = temp;

    }

    arr[0]=x;
    }
